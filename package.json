{
  "name": "frankgpt",
  "displayName": "FrankGPT",
  "description": "A DoD safer VSCode Extension for ChatGPT integration.",
  "version": "0.0.4",
  "publisher": "Frank Tudor",
  "license": "MIT",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./extension.js",
  "activationEvents": [
    "onCommand:frankgpt.helloWorld",
    "onCommand:frankgpt.askGPT",
    "onCommand:frankgpt.setApiKey",
    "onCommand:frankgpt.clearApiKey",
    "onCommand:frankgpt.selectModel"
  ],
  "contributes": {
    "commands": [
      {
        "command": "frankgpt.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "frankgpt.askGPT",
        "title": "Ask FrankGPT"
      },
      {
        "command": "frankgpt.setApiKey",
        "title": "Set API Key"
      },
      {
        "command": "frankgpt.clearApiKey",
        "title": "Clear API Key"
      },
      {
        "command": "frankgpt.selectModel",
        "title": "Select OpenAI Model"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.85.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.2"
  }
}
